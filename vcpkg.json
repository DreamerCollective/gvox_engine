{
  "name": "gvox-engine",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "daxa",
      "features": [
        "utils-pipeline-manager-spirv-validation",
        "utils-imgui",
        "utils-mem",
        "utils-pipeline-manager-glslang",
        "utils-task-graph"
      ]
    },
    "gvox",
    "fmt",
    "stb",
    "nlohmann-json",
    "platform-folders",
    "nativefiledialog",
    "minizip",
    "assimp",
    "freeimage",
    "glm",
    "soloud",
    {
      "name": "glfw3",
      "features": [
        "xlib",
        "wayland"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "docking-experimental"
      ]
    }
  ],
  "builtin-baseline": "78ba9711d30c64a6b40462c72f356c681e2255f3",
  "overrides": [
    {
      "name": "glfw3",
      "version-string": "custom"
    }
  ],
  "vcpkg-configuration": {
    "overlay-ports": [
      "deps/gvox/cmake/vcpkg-port",
      "deps/Daxa",
      "deps/Daxa/cmake/vcpkg-overlay-ports/dxc",
      "deps/Daxa/cmake/vcpkg-overlay-ports/glfw3",
      "deps/Daxa/cmake/vcpkg-overlay-ports/fsr2",
      "cmake/vcpkg-overlay-ports/fiber-tasking-lib",
      "cmake/vcpkg-overlay-ports/soloud",
      "cmake/vcpkg-overlay-ports/imgui-node-editor"
    ]
  }
}
