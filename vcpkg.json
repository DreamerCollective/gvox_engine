{
  "name": "my-app",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "daxa",
      "features": [
        "utils-pipeline-manager-spirv-validation"
      ]
    },
    "gvox",
    "fmt",
    "stb",
    "nlohmann-json",
    "platform-folders",
    "nativefiledialog",
    "minizip",
    {
      "name": "glfw3",
      "features": [
        "xlib",
        "wayland"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "docking-experimental"
      ]
    },
    "imgui-node-editor"
  ],
  "builtin-baseline": "959f8e317599dd439bc86f5cb5f0aca648d88c3f",
  "overrides": [
    {
      "name": "glfw3",
      "version-string": "custom"
    }
  ],
  "vcpkg-configuration": {
    "overlay-ports": [
      "../gvox/cmake/vcpkg-port",
      "../Daxa/cmake/vcpkg-port",
      "../Daxa/cmake/vcpkg-overlay-ports/dxc",
      "../Daxa/cmake/vcpkg-overlay-ports/glfw3",
      "../Daxa/cmake/vcpkg-overlay-ports/fsr2",
      "cmake/vcpkg-overlay-ports/imgui-node-editor"
    ]
  }
}
