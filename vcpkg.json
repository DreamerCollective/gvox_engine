{
  "name": "gvox-engine",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "daxa",
      "features": [
        "utils-pipeline-manager-spirv-validation"
      ]
    },
    "gvox",
    "fmt",
    "stb",
    "nlohmann-json",
    "platform-folders",
    "nativefiledialog",
    "minizip",
    "fiber-tasking-lib",
    {
      "name": "glfw3",
      "features": [
        "xlib",
        "wayland"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "docking-experimental"
      ]
    }
  ],
  "builtin-baseline": "0fa8459cf3a7caca7adc58f992bc32ff13630684",
  "overrides": [
    {
      "name": "glfw3",
      "version-string": "custom"
    }
  ],
  "vcpkg-configuration": {
    "overlay-ports": [
      "../gvox/cmake/vcpkg-port",
      "../Daxa/cmake/vcpkg-port",
      "../Daxa/cmake/vcpkg-overlay-ports/dxc",
      "../Daxa/cmake/vcpkg-overlay-ports/glfw3",
      "../Daxa/cmake/vcpkg-overlay-ports/fsr2",
      "cmake/vcpkg-overlay-ports/fiber-tasking-lib",
      "cmake/vcpkg-overlay-ports/imgui-node-editor"
    ]
  }
}
